			SCMF( id, 0x33CCFFFF, "(BRAK): {FFFFFF}%s {33CCFF}poslao/la ti je papire za razvod, pisi {FFFFFF}[ /accept divorce ] {33CCFF}da prihvatis.", ImeIgraca( playerid ) );

