//------------------------------------------------------------------------------
SendNearbyMessage(playerid, Float:radius, color, const str[], {Float,_}:...)
{
	static
	    args,
	    start,
	    end,
	    string[144];

	#emit LOAD.S.pri 8
	#emit STOR.pri args

	if(args > 16)
	{
		#emit ADDR.pri str
		#emit STOR.pri start

	    for(end = start + (args - 16); end > start; end -= 4)
		{
	        #emit LREF.pri end
	        #emit PUSH.pri
		}
		#emit PUSH.S str
		#emit PUSH.C 144
		#emit PUSH.C string

		#emit LOAD.S.pri 8
		#emit CONST.alt 4
		#emit SUB
		#emit PUSH.pri

		#emit SYSREQ.C format
		#emit LCTRL 5
		#emit SCTRL 4

        foreach(new i : Player)
		{
			if(IsPlayerNearPlayer(radius, playerid, i)) 
			{
  				SendClientMessage(i, color, string);
			}
		}
		return 1;
	}
	foreach(new i : Player)
	{
		if(IsPlayerNearPlayer(radius, playerid, i))
		{
			SendClientMessage(i, color, str);
		}
	}
	return 1;
}
