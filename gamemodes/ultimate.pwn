		PlayerInfo[ id ][ xPosao ] = kolicina;

		new q[ 128 ];
		mysql_format( mSQL, q, sizeof(q), "UPDATE `users` SET `job_id` = '%d' WHERE `user_id` = '%d' LIMIT 1", PlayerInfo[ id ][ xPosao ], PlayerInfo[ id ][ xID ] );
