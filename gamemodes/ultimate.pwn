					SetPlayerArmedWeapon( giveplayerid, weapon );

					SendNearbyMessage(playerid, 5.0, 0xC2A2DAFF, "* %s daje oruzje %s.", ImeIgraca( playerid ), ImeIgraca( giveplayerid ) );
				} 
				else SendErrorMessage( playerid, "Taj igrac nije blizu vas.");
			}
		} 
