			{
			    if( igrac == playerid ) return SendErrorMessage( playerid, "Ne mozes skiniti lisice sam sebi!");
				if( PlayerCuffed[ igrac ] == 2 )
				{
					SCMF( igrac, 0x33CCFFFF, "(CUFF): Skinio vam je lisice {FFFFFF}%s.", ImeIgraca( playerid ) );
