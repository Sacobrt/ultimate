	interior_info[ interior_counter ][ int_IntEnter ] = IntIDx;
	interior_info[ interior_counter ][ int_VwEnter ] = vwIDx;

	new string[100];
	format(string, sizeof(string), "{4282C0}(( %s ))\n{FFFFFF}\"ENTER\" {4282C0}za ulaz.", name);
