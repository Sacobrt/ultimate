    TrashTimer[ playerid ] = SetPlayerTimerEx(playerid, "TrashLimitOff", 25000, false, "i", playerid);
    ApplyAnimation(playerid,"PED","IDLE_tired",3.0,1,0,0,0,0);
	return 1;
}

