	if( PlayerInfo[ playerid ][ xAdmin ] >= 2 )
	{
		new id, vreme, razlog[40];
		if( sscanf( params, "uis[40]", id, vreme, razlog ) )
		{
			SendUsageMessage( playerid, "/prison [id] [sati] [razlog]");
