{
	if( veh_model < 400 || veh_model > 611 ) return 1;

	new i = IVI;

    i = SMG_CreateVehicle( veh_model, veh_pos_x, veh_pos_y, veh_pos_z+1.0, veh_pos_a, veh_col_1, veh_col_2, 300000 );

