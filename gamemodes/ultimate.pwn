	    				new time = gettime( );
		       			new ostatak = ( NovoVreme[ playerid ] - ( time - StaroVreme[ playerid ] ) );
			        	if( ( time - StaroVreme[ playerid ] ) < NovoVreme[ playerid ] )
						{
