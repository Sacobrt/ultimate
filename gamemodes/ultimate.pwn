     			{
					new weapon = GetWeapon( playerid );

                    if( GetFactionType( playerid ) == ORG_TIP_VLADINA || GetFactionType( playerid ) == ORG_TIP_HITMAN ) {
				        SendErrorMessage( playerid, "PD/SD/HITMAN nema razloga davati oruzja!");
