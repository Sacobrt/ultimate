CMD:offplayerveh( playerid, const params[] )
{
    if( PlayerInfo[ playerid ][ xAdmin ] >= 6  )
	{
		new imeigraca[MAX_PLAYER_NAME];
		if( sscanf( params, "s[24]", imeigraca ) ) return SendUsageMessage( playerid, "/(opv)offplayerveh [Ime_Prezime]" );

