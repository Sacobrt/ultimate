	if( x <= gZoneInfo[ id ][ gZoneMaxX ] && x >= gZoneInfo[ id ][ gZoneMinX ] && y <= gZoneInfo[ id ][ gZoneMaxY ] && y >= gZoneInfo[ id ][ gZoneMinY ] ) 
